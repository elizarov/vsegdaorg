<queue-entries>
    <queue>
        <name>archiveTaskQueue</name>
        <rate>10/m</rate>
        <bucket-size>1</bucket-size>
        <max-concurrent-requests>1</max-concurrent-requests>
        <retry-parameters>
            <task-retry-limit>1</task-retry-limit>
            <task-age-limit>15m</task-age-limit>
        </retry-parameters>
    </queue>
    <queue>
        <name>dataCacheRefreshTaskQueue</name>
        <rate>10/m</rate>
        <bucket-size>1</bucket-size>
        <max-concurrent-requests>1</max-concurrent-requests>
        <retry-parameters>
            <task-retry-limit>1</task-retry-limit>
            <task-age-limit>15m</task-age-limit>
        </retry-parameters>
    </queue>
</queue-entries>